<?xml version="1.0" encoding="UTF-8" ?>
<Shell xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
       xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
       xmlns:local="clr-namespace:pw2"
       x:Class="pw2.AppShell">

    <ShellContent Title="Register" ContentTemplate="{DataTemplate local:LoginPage}" Route="LoginPage" />

    <ShellContent Title="Log In" ContentTemplate="{DataTemplate local:LoginRealPage}" Route="LoginRealPage" />

    <ShellContent Title="User Info" ContentTemplate="{DataTemplate local:OperationsPage}" Route="OperationsPage" />

    <ShellContent Title="Converter" ContentTemplate="{DataTemplate local:ConversorPage}" Route="ConversorPage" />


</Shell>
